{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "DynamoPDF",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373"
  },
  "favicon": "/favicon.ico",
  "api": {
    "playground": {
      "display": "interactive",
      "proxy": true
    },
    "examples": {
      "languages": ["curl", "javascript", "python", "php", "Java", "go"],
      "defaults": "required"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "PDF Generation",
            "pages": [
              "api-reference/auto-generated/pdf-generation/generate-pdf",
              "api-reference/auto-generated/pdf-generation/generate-pdfs-batch"
            ]
          },
          {
            "group": "Templates",
            "pages": [
              "api-reference/auto-generated/templates/list-templates",
              "api-reference/auto-generated/templates/get-template"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://app.dynamopdf.com"
      }
    ],
    "primary": {
      "type": "button",
